  "parameters": [
    {
      "name": "loaded-modelid",
      "initialValue": "water0"
    },
    {
      "name": "h2-param",
      "label": "H2",
      "onChange": [
        "var o2 = get('o2-param') || 0,",
        "    num = +get('h2-param') * 10 + o2,",
        "    modelid = 'water' + num;",
        "set('loaded-modelid', modelid);"
      ],
      "initialValue": 0
    },
    {
      "name": "o2-param",
      "label": "O2",
      "onChange": [
        "var o2 = +get('o2-param') || 0,",
        "    num = +get('h2-param') * 10 + o2,",
        "    modelid = 'water' + num;",
        "set('loaded-modelid', modelid);"
      ],
      "initialValue": 0
    }
  ],
  "components": [
